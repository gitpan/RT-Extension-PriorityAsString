<span class="ticket-info-final-priority-<% lc($final) %>"><% loc($final) %></span>
<%ARGS>
$Ticket => undef
</%ARGS>
<%INIT>
my $current = $Ticket->PriorityAsString;
my $final = $Ticket->FinalPriorityAsString;
</%INIT>
